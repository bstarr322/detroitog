window.Detroitog = window.Detroitog || {};

var Detroitog = {
  init: function() {
    this.globalEvents();
    this.headerEvents();
    this.homePageEvents();
    this.gridViewEvents();
    this.productPageEvents();
    this.passwordPageHeader();
  },

  globalEvents: function() {

    $(".product_images img").unveil(200);

    "use strict";

    /////////////////////////////////////
    //  animate elements when they are in viewport
    /////////////////////////////////////

    $('.noIE .animated').waypoint(function () {
      var animation = $(this).data('animation');
      $(this).addClass('animation-done').addClass(animation);
    }, {
      triggerOnce: true,
      offset: '70%'
    });

    $('[data-toggle="tooltip"]').tooltip();

    $('#plugdetroit').on('mouseenter', function() {
      $(this).animateCssOriginal('jello')
    });

    $(document).on({
      mouseenter: function (evt) {
        $(evt.target).data('hovering', true);
      },
      mouseleave: function (evt) {
        $(evt.target).data('hovering', false);
      }
    }, '*');
    
    $(window).scroll(function() {
      var wintop = $(window).scrollTop(), docheight = $(document).height(), footerDistance = $('footer').height(), winheight = $(window).height();
      var totalScroll = (wintop/(docheight - footerDistance - winheight))*100;
      $(".StickyFooter-progress").css("width",totalScroll+"%");
    });
    
    // Footer Events
    $('.footer_scrollup').click(function(){
      $("html, body").animate({ scrollTop: 0 }, 600);
      return false;
    });

    // Hide Tidio chat when Right Drawer is opened.
    $('body').on('afterDrawerOpen.timber', function() {
      if (!$(this).hasClass('js-drawer-open-left')) {
        $('#tidio-chat').addClass('hide');
      }
    }).on('afterDrawerClose.timber', function() {
      $('#tidio-chat').removeClass('hide');
    })
  },

  headerEvents: function() {
    $('.account-open').click(function() {
      $(this).closest('.header-options').fadeOut(100, function() {
        return $('.account-container').fadeIn(200);
      });
      return false;
    });
    $('.account-close').click(function() {
      $('.account-container').fadeOut(100, function() {
        return $('.header-options').fadeIn(200);
      });
      return false;
    });

    // Sticky Header 
    $(".site-header").sticky({topSpacing:0});

    // Header Dropdown
    $('.header-collections__menu--dropdown').on('click', function() {
      $(this).siblings().removeClass('dropdown-opened');
      $(this).toggleClass('dropdown-opened');
    });
  },

  homePageEvents: function() {
    $('.home-slider').flexslider({
      animation: 'fade', //String: Select your animation type, "fade" or "slide"
      //Boolean: Create navigation for previous/next navigation? (true/false)
      slideshowSpeed: 5000, //Integer: Set the speed of the slideshow cycling, in milliseconds
      animationSpeed: 600, //Integer: Set the speed of animations, in milliseconds
      pauseOnHover: false, //Boolean: Pause the slideshow when hovering over slider, then resume when no longer hovering
      prevText: "", //String: Set the text for the "previous" directionNav item
      nextText: "" //String: Set the text for the "next" directionNav item
    });

    $(".flex-control-nav").prepend('<a class="prev" href="/"><span class="fa fa-chevron-left"></span></a>');
    $(".flex-control-nav").append('<a class="next" href="/"><span class="fa fa-chevron-right"></span></a>');


    $(".flex-control-nav .prev").click(function () {
        $(".flex-prev").trigger('click');
    });
    $(".flex-control-nav .next").click(function () {
        $(".flex-next").trigger('click');
    });

    $(".flex-control-nav").wrap("<div class='container'></div>");
  },

  gridViewEvents: function() {

    // Fire Magnific Popup for Quick view
    // Quick View will be based on 2 types - inline, iframe.
    // - inline is for Related Products, Normal Products Grid
    // - iframe is for Recently Viewed Products

    var QuickViewBtns = $('a.quick-view-btn'); /* find all the the quick view launch buttons */
    if(QuickViewBtns.length){

      // setup the quickview popup action
      QuickViewBtns.magnificPopup({
        type:'inline',
        midClick: true,
        mainClass:'mfp-move-from-top',
        callbacks : {
          open : function(){
            var mp = $.magnificPopup.instance;
            var cur = mp.st.el;
            var href = $(cur).attr('href');
            
            var data = $('.mfp-wrap .quick-view-panel').data();
            $('.product-sku').css('display', 'none');
            
            Detroitog.productPageEvents();
            $(window).trigger('resize');

            setTimeout(function() {

              // Unchecked when make a selection.    
              var radios = jQuery('.product-form__item--swatch').find("input[type='radio']");
              for ( i = 0; i < radios.length; i++ ) {
                radios.get(i).checked = false;
              }

              // Make a Selection
              if (data.variantSize > 1) {
                $('#AddToCart').addClass('disabled').attr('disabled', 'disabled');
                $('#AddToCart').html('<span>Make a Selection</span>');
                $('.product-form__bis').addClass('hide_bis');
                if (!data.available){
                  $('#AddToCart').show();
                }
              } else {
                $('.product-sku').css('display', 'inline-block');
                if (!data.available){
                  $('#AddToCart').addClass('disabled').hide();
                  $('.product-form__bis').removeClass('hide_bis');
                }
              }

            }, 100);
          }
        }
      });
    }

  },

  productPageEvents: function() {
    
     var galleryTop = new Swiper('#product_gallery', {
      spaceBetween: 10,
      allowTouchMove: true,
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
    });
    window.galleryThumbs = new Swiper('#ProductThumbs', {
      allowTouchMove: true,
      slidesPerView: 4,
      spaceBetween: 10,
      touchRatio: 0.2,
      slideToClickedSlide: true,
      breakpoints: {
        1024: {
          slidesPerView: 3,
        },
        640: {
          slidesPerView: 2,
        }
      }
    });
    galleryTop.controller.control = galleryThumbs;
    galleryThumbs.controller.control = galleryTop;

    // Initialize Product Main Photo Popup
    $('.product_gallery.nomagnific .slides').magnificPopup({
      delegate: 'a',
      type: 'image',
      closeOnContentClick: false,
      closeBtnInside: false,
      mainClass: 'mfp-with-zoom mfp-img-mobile',
      image: {
        verticalFit: true
      },
      gallery: {
        enabled: true
      },
      zoom: {
        enabled: true,
        duration: 300, // don't foget to change the duration also in CSS
        opener: function(element) {
          return element.find('img');
        }
      }
    });

    // Product Grid Slider, we will use swiper slider.
    var swiper = new Swiper('.related-products .swiper-container', {
      slidesPerView: 4,
      spaceBetween: 10,
      // init: false,
      pagination: {
        el: '.swiper-pagination',
        clickable: true,
      },
      breakpoints: {
        1024: {
          slidesPerView: 3,
          spaceBetween: 10,
        },
        768: {
          slidesPerView: 2,
          spaceBetween: 10,
        },
        640: {
          slidesPerView: 2,
          spaceBetween: 10,
        },
        480: {
          slidesPerView: 1,
          spaceBetween: 10,
        }
      }
    });

    {% if settings.show_product_zoom %}
    /////////////////////////////////////
    //  Product Image Zoom
    /////////////////////////////////////
    if(typeof $.zoom === 'function'){
      var $mainPhotoWrapper = $('#ProductPhoto');
      if($mainPhotoWrapper.length){
        var $imageSlideLinks = $mainPhotoWrapper.find('a.image-slide-link');
        if($imageSlideLinks.length){
          $imageSlideLinks.each(function(){
            var t = $(this);
            var url = t[0].href;
            t.zoom({ url:url,touch:false });
          });
        }
      }
    }
    {% endif %}
  },

  passwordPageHeader: function() {
    $('.js-toggle-login-modal').magnificPopup({
      type: 'inline',
      mainClass: 'mfp-fade',
      closeOnBgClick: false,
      closeBtnInside: false,
      closeOnContentClick: false,
      // tClose: password.strings.pageClose,
      removalDelay: 500,
      callbacks: {
        open: function() {
          window.setTimeout(function() {
            document.getElementById('password').focus();
          }, 50);
        },
        close: function() {
          window.setTimeout(function() {
            document.getElementById('email').focus();
          }, 50);
        }
      }
    });
    if ($('.storefront-password-form .errors').size()) {
      $('.js-toggle-login-modal').click();
    }
  }
}

$.fn.extend({
  animateCss: function (animationName) {
    var animationEnd = 'webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend';
    this.addClass('animated ' + animationName).one(animationEnd, function() {
      $(this).removeClass('animated ' + animationName);
    });
    return this;
  }
});


jQuery(function() {
  Detroitog.init();
}); 