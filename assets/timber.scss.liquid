    border-right: 0;
    border-left: 0;
    border-radius: 0;
    margin: 0;
  }
}

.js-qty__adjust {
  cursor: pointer;
  display: block;
  width: 37px;
  height: 37px;
  line-height: 37px;
  font-size: 14px;
  font-weight: 400;
  color: #000000;
  border: 0 none;
  padding: 0 8px;
  background: transparent;
  text-align: center;
  overflow: hidden;
  outline: none;
  @include user-select(none);

  &:hover,
  &:focus {
    color: #fff;
    background-color: $colorAccent;
  }
}

.js-qty__adjust--plus {
  float: left;
  border: 1px solid $colorAccent;
  border-radius: 0 20px 20px 0;
}

.js-qty__adjust--minus {
  float: left;
  border: 1px solid $colorAccent;
  border-radius: 20px 0 0 20px;
}

/*================ Quantity Selectors in Ajax Cart ================*/
.ajaxcart__qty {
  @extend .js-qty;
  margin: 0;

  .is-loading & {
    opacity: 0.5;
    transition: none;
  }
}

.ajaxcart__qty-num {
  border-color: $colorDrawerBorder;
  color: $colorDrawerText;
}

.ajaxcart__qty-adjust {
  @extend .js-qty__adjust;
  float: left;
  color: $colorDrawerText;
}

.ajaxcart__qty-adjust.ajaxcart__qty--minus {
  border: 1px solid $colorAccent;
  border-radius: 20px 0 0 20px;
  width: 33px;
  height: 33px;
  line-height: 33px;
}

.ajaxcart__qty-num {
  width: 42px !important;
  height: 33px !important;
  line-height: 33px !important;
}

.ajaxcart__qty-adjust.ajaxcart__qty--plus {
  border: 1px solid $colorAccent;
  border-radius: 0 20px 20px 0;
  width: 33px;
  height: 33px;
  line-height: 33px;
}

.ajaxcart-item__price-strikethrough {
  float: right;
}

{% endif %} // settings.ajax_cart_method


/* Custom Variables  */
$font-size-small: 12px;
$border-radius-base: 4px;

$zindex-tooltip:           1070 !default;

$tooltip-max-width:           200px !default;
$tooltip-color:               #fff !default;
$tooltip-bg:                  #000 !default;
$tooltip-opacity:             .9 !default;
$tooltip-arrow-width:         5px !default;
$tooltip-arrow-color:         $tooltip-bg !default;


.tooltip {
  position: absolute;
  z-index: $zindex-tooltip;
  display: block;
  font-size: $font-size-small;
  font-weight: bold;
  animation-duration: .1s;
  animation-delay: .2s;

  &.top    { margin-top:  -3px; padding: $tooltip-arrow-width 0; }
  &.right  { margin-left:  3px; padding: 0 $tooltip-arrow-width; }
  &.bottom { margin-top:   3px; padding: $tooltip-arrow-width 0; }
  &.left   { margin-left: -3px; padding: 0 $tooltip-arrow-width; }
}

.tooltip-inner {
  max-width: $tooltip-max-width;
  padding: 3px 8px;
  color: $tooltip-color;
  text-align: center;
  background-color: $tooltip-bg;
  border-radius: $border-radius-base;
}

.tooltip-arrow {
  position: absolute;
  width: 0;
  height: 0;
  border-color: transparent;
  border-style: solid;
}

.tooltip {
  &.top .tooltip-arrow {
    bottom: 0;
    left: 50%;
    margin-left: -$tooltip-arrow-width;
    border-width: $tooltip-arrow-width $tooltip-arrow-width 0;
    border-top-color: $tooltip-arrow-color;
  }
  &.right .tooltip-arrow {
    top: 50%;
    left: 0;
    margin-top: -$tooltip-arrow-width;
    border-width: $tooltip-arrow-width $tooltip-arrow-width $tooltip-arrow-width 0;
    border-right-color: $tooltip-arrow-color;
  }
  &.left .tooltip-arrow {
    top: 50%;
    right: 0;
    margin-top: -$tooltip-arrow-width;
    border-width: $tooltip-arrow-width 0 $tooltip-arrow-width $tooltip-arrow-width;
    border-left-color: $tooltip-arrow-color;
  }
  &.bottom .tooltip-arrow {
    top: 0;
    left: 50%;
    margin-left: -$tooltip-arrow-width;
    border-width: 0 $tooltip-arrow-width $tooltip-arrow-width;
    border-bottom-color: $tooltip-arrow-color;
  }
}

// Frequently Bought Together Custom Styles
.cbb-frequently-bought-container {
  .money {
    color: $colorAccent;
    font-weight: bold;
    font-size: 18px;
  }
  .cbb-frequently-bought-add-button {
    color: #fff;
    background-color: $colorAccent;
    text-transform: uppercase;
    position: relative;
    padding: 15px 20px;

    &:after {
      content: '';
      position: absolute;
      bottom: 0;
      left: -100%;
      width: 100%;
      height: 52px;
      padding: 8px;
      border-radius: 3px;
      background-image: -webkit-linear-gradient(left, rgba(255, 255, 255, 0) 0%, white 50%, rgba(255, 255, 255, 0) 100%);
      background-image: linear-gradient(to right, rgba(255, 255, 255, 0) 0%, white 50%, rgba(255, 255, 255, 0) 100%);
      -webkit-animation: sheen 1.0s ease 0.0s infinite;
      animation: sheen 1.0s ease 0.0s infinite;
      opacity: 0.2;
    }
  }

  @include at-query ($max, $small) {
    .cbb-frequently-bought-products {
      max-width: 100%;
    }

    .cbb-frequently-bought-form {
      width: 100%;
    }
  }
}

.instashow-gallery {
  z-index: 1;
}

#snize-sticky-searchbox-searchbox {
  display: none !important;
}




